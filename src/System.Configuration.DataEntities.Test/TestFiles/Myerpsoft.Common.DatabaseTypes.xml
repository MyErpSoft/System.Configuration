<?xml version="1.0" encoding="utf-8" ?>
<!-- namespace Define：To prevent naming conflicts, different developers isolate results -->
<!-- assembly  Define：Realization of lazy loading, when an object pointer is found by checking the Assembly delay load -->
<!-- Product   Define：Poor implementation of quantitative methods, by defining a product dependency obtain the correction results -->
<Metadata version="1.1">

  <!-- define a namespace -->
  <Namespace name="Myerpsoft.Common.DatabaseTypes">

    <!-- Define a object instance.(not is class),DatabaseType is class type,  -->
    <DatabaseType name="Int32">

      <!-- set object property value. -->
      <DbTpe>int</DbTpe>

      <!-- add item to collection property. -->
      <SpecificTypes>
        <!-- using a object ref -->
        <Item Ref="Oracle.Int32" />
        <Item Ref="Oracle.11g.Int32" />
      </SpecificTypes>
    </DatabaseType>

    <!-- Nested namespaces -->
    <Namespace name="Oracle">

      <!-- This 'base' to emulate inheritance of classes, but here is an instance of the object value strategy -->
      <DatabaseType name="Int32" base="Int32">

        <!-- override base object property value -->
        <DbType>number</DbType>

        <!-- base object not set 'Size[int?]' property,is null, this override is 38.-->
        <Size>38</Size>

        <ProductName>Oracle</ProductName>
      </DatabaseType>

      <Namespace name="11g">
        <DatabaseType name="Int32" base="Int32">
          <DbType>BINARY_INTEGER</DbType>

          <!-- override 'Size[int?]' property is null -->
          <Size />
          <ProductName>Oracle</ProductName>
          <Version>11g</Version>
        </DatabaseType>
      </Namespace>

    </Namespace>

  </Namespace>

</Metadata>