<?xml version="1.0" encoding="utf-8" ?>
<x:ObjectContainer
               xmlns="clr-namespace:System.Configuration.Core.Tests,System.Configuration.Core.Tests"
               xmlns:x="http://schemas.myerpsoft.com/configuration/2015"
               x:namespace="company.erp.demo">

  <!--有一个按钮，集合包含2项。-->
  <Button x:name="A1" >
    <Text>A1</Text>
    <Controls>
      <Button x:ref="A1_1" />
      <Button x:ref="A1_2" />
    </Controls>
  </Button>

  <Button x:name="A1_1" >
    <Text>A1_1</Text>
  </Button>

  <Button x:name="A1_2" >
    <Text>A1_2</Text>
  </Button>

  <!-- 派生自A1，增加和删除项目 -->
  <Button x:name="B1" x:base="A1">
    <Text>B1</Text>
    <Controls>
      <Button x:ref="A1_2" x:action="remove" />
      <Button x:ref="B1_1" />
    </Controls>
  </Button>

  <Button x:name="B1_1" >
    <Text>B1_1</Text>
  </Button>

  <Button x:name="C1_1" >
    <Text>C1_1</Text>
  </Button>

  <Button x:name="D1_1" >
    <Text>D1_1</Text>
  </Button>
  
  <Button x:name="C1" >
    <Text>C1</Text>
    <Controls>
      <Button x:ref="A1_1" />
      <Button x:ref="A1_2" />
      <Button x:ref="B1_1" />
      <Button x:ref="C1_1" />
    </Controls>
  </Button>

  <!--测试代码中触发HashSet的使用。（DifferenceList.GetList）-->
  <Button x:name="D1" x:base="C1">
    <Text>D1</Text>
    <Controls>
      <Button x:ref="A1_1" x:action="remove"/>
      <Button x:ref="A1_2" x:action="remove"/>
      <Button x:ref="B1_1" x:action="remove"/>
      <Button x:ref="C1_1" x:action="remove"/>
      <Button x:ref="D1_1" />
    </Controls>
  </Button>
  
</x:ObjectContainer>